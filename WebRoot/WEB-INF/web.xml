<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>VisitorServlet</servlet-name>
    <servlet-class>com.servlet.VisitorServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>RootLoginServlet</servlet-name>
    <servlet-class>com.servlet.RootLoginServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>IndexServlet</servlet-name>
    <servlet-class>com.servlet.IndexServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LogoutServlet</servlet-name>
    <servlet-class>com.servlet.LogoutServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>VisitorServlet</servlet-name>
    <url-pattern>/servlet/VisitorServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RootLoginServlet</servlet-name>
    <url-pattern>/servlet/RootLoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>IndexServlet</servlet-name>
    <url-pattern>/servlet/IndexServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LogoutServlet</servlet-name>
    <url-pattern>/servlet/LogoutServlet</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>changeVisitorInformation</display-name>
    <servlet-name>changeVisitorInformation</servlet-name>
    <servlet-class>com.servlet.changeVisitorInformation</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>changeVisitorInformation</servlet-name>
    <url-pattern>/changeVisitorInformation</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>changeInformation</display-name>
    <servlet-name>changeInformation</servlet-name>
    <servlet-class>com.servlet.changeInformation</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>changeInformation</servlet-name>
    <url-pattern>/changeInformation</url-pattern>
  </servlet-mapping>
</web-app>